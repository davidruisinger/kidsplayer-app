{
  "expo": {
    "name": "little-spotify",
    "slug": "little-spotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "little-spotify",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flavordaaave.little-spotify"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.flavordaaave.littlespotify",
      "permissions": [
        "android.permission.NFC"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-nfc-manager",
      [
        "./modules/little-spotify/app.plugin.js",
        {
          "clientID": "c105950d5dcf4b3aba8e1238e1a3a908",
          "scheme": "little-spotify",
          "host": "spotify-auth"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "10908e79-5766-4c67-b4cf-a723b75adc57"
      }
    }
  }
}
